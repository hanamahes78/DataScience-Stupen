'brute'],\n","    'leaf_size': [30, 50, 70],\n","    'n_neighbors' : [3, 5, 7]\n","}\n","\n","# Buat objek klasifikasi KNN\n","modelKNN = KNeighborsClassifier()\n","\n","# Buat objek GridSearchCV\n","grid_search = GridSearchCV(\n","    modelKNN,\n","    hyperparameters,\n","    cv = 5,\n","    scoring = 'accuracy'\n",")\n","\n","# Run proses hyperparameter tuning\n","grid_search.fit(X_resampled_scaled, y_resampled)\n","\n","# Print hyperparameter terbaik dan skor terbaik\n","print(\"Best Hyperparameters: \", grid_search.best_params_)\n","print(\"Best Score: {:.2f}\".format(grid_search.best_score_))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1712665895873,"user_tz":-420,"elapsed":3201,"user":{"displayName":"Hana Maheswari","userId":"16347520931021608502"}},"outputId":"ce2b72c4-53a8-4f0f-a11d-6c88a912dcda","id":"-nokr7zIMCFD"},"execution_count":51,"outputs":[{"output_type":"stream","name":"stdout","text":["Best Hyperparameters:  {'algorithm': 'auto', 'leaf_size': 30, 'n_neighbors': 3}\n","Best Score: 0.91\n"]}]},{"cell_type":"markdown","source":["## **10.5 Fitting Model dengan Parameter Hasil Tunning**"],"metadata":{"id":"qg516fGFMCFE"}},{"cell_type":"code","source":["from sklearn.neighbors import KNeighborsClassifier\n","from sklearn.preprocessing import StandardScaler\n","\n","# Penskalaan fitur\n","scaler = StandardScaler()\n","X_resampled_scaled = scaler.fit_transform(X_resampled)\n","\n","# Membuat objek klasifikasi KNN dengan hyperparameters terbaik\n","modelKNN_baru = KNeighborsClassifier(\n","    algorithm = 'auto',\n","    leaf_size = 30,\n","    n_neighbors = 3\n",")\n","\n","# Melatih model dengan data training yang telah di-scaled\n","modelKNN_baru.fit(X_resampled_scaled, y_resampled)\n","\n","# Melakukan prediksi pada data training\n","y_pred_resampled = modelKNN_baru.predict(X_resampled_scaled)"],"metadata":{"executionInfo":{"status":"ok","timestamp":1712665897099,"user_tz":-420,"elapsed":644,"